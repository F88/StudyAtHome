{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    // "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@types/node", "@types/express", "@nuxt/types"]
  },
  "compileOnSave": true,
  "include": ["src"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
